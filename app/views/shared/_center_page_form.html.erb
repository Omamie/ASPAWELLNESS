
<div class="container py-3 ">
  <div class="modal-card-smaller p-3">
    <div>
      <h1 class="mx-3 my-3"> Edit Your Page </h1>
      <div>
        <%= simple_form_for [@center, @photo] do |f| %>
       <%= f.input :name %>
        <%= f.input :address %>
        <%= f.input :contact_person %>
        <%= f.photos :photos %>
        <%= f.submit "Add Photos", class: "btn btn-primary" %>
      </div>
      <% end %>
    </div>
  </div>

