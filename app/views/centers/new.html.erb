<div class="container py-3 ">
  <div class="modal-card-smaller p-3">
    <div>
      <h1 class="mx-3 my-3"> Register your center </h1>
      <div>
        <%= simple_form_for (@center) do |f| %>
        <%= f.input :name %>
        <%= f.input :contact_person %>
        <%= f.input :address %>
        <%= f.submit "Register now", class: "btn btn-primary" %>
      </div>
      <% end %>
    </div>
  </div>
